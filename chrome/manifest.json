{
    "content_scripts": [
        {
            "all_frames": true,
            "js": [
                "nonnav.js"
            ],
            "matches": [
                "*://*.pardus.at/game.php",
                "*://*.pardus.at/menu.php",
                "*://*.pardus.at/msgframe.php"
            ],
            "run_at": "document_end"
        },
        {
            "all_frames": true,
            "js": [
                "storage.js",
                "main.js",
                "ui.js",
                "sgpvp.js"
            ],
            "css": [
                "ui.css"
            ],
            "matches": [
                "*://*.pardus.at/*"
            ],
            "exclude_matches": [
                "*://*.pardus.at/game.php",
                "*://*.pardus.at/menu.php",
                "*://*.pardus.at/msgframe.php"
            ],
            "run_at": "document_end"
        }
    ],
    "content_security_policy": {
        "extension_pages": "default-src 'self'"
    },
    "description": "Keyboard shortcuts for the browser game Pardus",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "manifest_version": 3,
    "name": "Scorpion Guard's Better PvP for Pardus",
    "permissions": [
        "storage"
    ],
    "short_name": "SGPvP",
    "web_accessible_resources": [
        {
            "resources": [
                "ui.html",
                "ui.css",
                "default-keymap.json",
                "postvars.js",
                "help.html",
                "help.css",
                "changelog.html",
                "customkeys.html"
            ],
            "matches": [
                "*://*.pardus.at/*"
            ]
        }
    ],
    "version": "45"
}